<Parallel>
    <cmd unix="ls -l"   windows="DIR"/>
    <cmd unix="ls -tr"  windows="DIR *.* /o-d"/>
    <cmd unix="ls -ls"  windows="DIR *.* /v /os"/>
    <cmd unix="ls -R"   windows="DIR /s"/>
    <cmd unix="ls -a"   windows="DIR /aa"/>
    <cmd unix="ls -R"   windows="TREE"/>
    <cmd unix="cd"      windows="CD"/>
    <cmd unix="mkdir"   windows="MKDIR MD"/>
    <cmd unix="ln"      windows="ASSIGN"/>
    <cmd unix="rmdir"   windows="RMDIR RD"/>
    <cmd unix="pwd"     windows="CHDIR"/>
    <cmd unix="rm -iv"  windows="DEL ERASE"/>
    <cmd unix="rm -R"   windows="RMDIR /S DELTREE"/>
    <cmd unix="COPY"    windows="cp -piv"/>
    <cmd unix="XCOPY"   windows="cp -R"/>
    <cmd unix="RENAME"  windows="mv -iv"/>
    <cmd unix="MOVE"    windows="mv -iv"/>
    <cmd unix="TYPE"    windows="cat"/>
    <cmd unix="MORE"    windows="more"/>
    <cmd unix="HELP"    windows="man"/>
    <cmd unix="COMMAND /?" windows="man"/>
    <cmd unix="CLS"     windows="clear"/>
    <cmd unix="EXIT"    windows="exit"/>
    <cmd unix="EXIT0"   windows="exit 0"/>
    <cmd unix="FIND"    windows="grep"/>
    <cmd unix="FINDSTR" windows="grep"/>
    <cmd unix="COMP"    windows="diff"/>
    <cmd unix="FC"      windows="diff"/>
    <cmd unix="SET"     windows="set"/>
    <cmd unix="ENV"     windows="env"/>
    <cmd unix="ECHO"    windows="echo"/>
    <cmd unix="DATE"    windows="date"/>
    <cmd unix="TIME"    windows="date"/>

</Parallel>
